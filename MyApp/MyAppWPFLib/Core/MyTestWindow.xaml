<Window x:Class="MyAppWPF.Core.MyTestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyAppWPF.Core"
        mc:Ignorable="d"
        Title="MyTestWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button x:Name="btnLoad" Click="btnLoad_Click" Content="Load"></Button>
            <DataGrid x:Name="MyGrid" AutoGenerateColumns="True" ItemsSource="{Binding MyObervableList}"></DataGrid>
        </StackPanel>
    </Grid>
</Window>
